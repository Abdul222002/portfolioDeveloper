---
import Layout from '../layouts/Layout.astro';
const { default: meData } = await import('../assets/me.json');
---
<Layout>
  <h1>{meData.nombre}</h1>
  <p>{meData.profesion}</p>
  <p>{meData.bio}</p>
  <section>
    <h2>Experiencia</h2>
    <ul>
      {meData.experiencia.map((exp) => (
        <p>{exp.nombre}</p>
        <p>{exp.descripcion}</p>
        <p>{exp.fecha}</p>
      ))}
    </ul>
  </section>

  <section>
    <h2>Habilidades</h2>
    <ul>
      {meData.habilidades.map((habilidad) => (
        <p>{habilidad}</p>
      ))}
  </section>
</Layout>
 
